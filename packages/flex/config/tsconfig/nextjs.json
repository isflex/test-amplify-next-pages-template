{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.cjs.json",
  "compilerOptions": {
    "module": "ES2022",
    "target": "es6",
    "lib": ["ES2022","dom","dom.iterable"],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "strict": false,
    "noEmit": true
  },
  "include": ["src", "next-env.d.ts"],
  "exclude": ["node_modules"]
}
